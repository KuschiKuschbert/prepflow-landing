{
  "name": "prepflow-landing",
  "version": "0.1.1",
  "private": true,
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "lint-staged",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "node scripts/analyze-bundle.js",
    "lighthouse": "lhci autorun",
    "lighthouse:mobile": "lighthouse http://localhost:3000 --view --emulated-form-factor=mobile",
    "lighthouse:desktop": "lighthouse http://localhost:3000 --view --emulated-form-factor=desktop",
    "perf:test": "npm run build && npm run lighthouse",
    "perf:analyze": "npm run build && npm run analyze",
    "bundle:analyze": "node scripts/analyze-bundle.js",
    "bundle:webpack": "ANALYZE=true npm run build",
    "audit:deps": "node scripts/audit-dependencies.js",
    "bundle:size": "npm run build && npm run analyze",
    "lint:filesize": "node scripts/check-file-sizes.js",
    "changelog": "node scripts/generate-changelog.js",
    "detect-breakpoints": "node scripts/detect-breakpoints.js",
    "codemod:breakpoints": "npx jscodeshift -t scripts/codemods/breakpoint-migration.js app components",
    "codemod:breakpoints:write": "npx jscodeshift -t scripts/codemods/breakpoint-migration.js --write app components",
    "codemod:console": "npx jscodeshift -t scripts/codemods/console-migration.js app components",
    "codemod:console:write": "npx jscodeshift -t scripts/codemods/console-migration.js --write app components",
    "codemod:all": "npm run codemod:breakpoints && npm run codemod:console",
    "cleanup": "node scripts/cleanup.js",
    "cleanup:check": "node scripts/cleanup.js --check",
    "cleanup:fix": "node scripts/cleanup.js --fix",
    "cleanup:report": "node scripts/cleanup.js --report",
    "cleanup:all": "node scripts/cleanup.js --check --fix --report",
    "cleanup:staged": "node scripts/cleanup.js --staged",
    "pre-deploy": "bash scripts/pre-deploy-check.sh",
    "find-allergen-conflicts": "node scripts/find-allergen-vegan-conflicts.js",
    "supabase:status": "npx supabase status",
    "supabase:migrate": "npx supabase db push",
    "supabase:migrate:reset": "npx supabase db reset",
    "supabase:migrate:clean": "npx supabase migration repair --status reverted",
    "supabase:link": "npx supabase link --project-ref $(echo $NEXT_PUBLIC_SUPABASE_URL | sed 's|https://\\([^.]*\\)\\.supabase\\.co|\\1|')",
    "supabase:diff": "npx supabase db diff",
    "supabase:generate": "npx supabase migration new",
    "supabase:migrate:cleaning": "bash scripts/supabase-migrate-cleaning.sh",
    "supabase:verify:cleaning": "node scripts/verify-cleaning-migration.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "2.78.0",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-query-devtools": "^5.87.4",
    "@types/howler": "^2.2.12",
    "@types/qrcode": "^1.5.6",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "canvas-confetti": "^1.9.4",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "googleapis": "^144.0.0",
    "gtag": "^1.0.1",
    "howler": "^2.2.4",
    "lucide-react": "^0.553.0",
    "next": "^16.0.0",
    "next-auth": "^4.24.13",
    "openai": "^6.9.0",
    "qrcode": "^1.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-qr-code": "^2.0.18",
    "recharts": "^2.8.0",
    "stripe": "^19.2.0",
    "three": "^0.181.2",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "16.0.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/canvas-confetti": "^1.9.0",
    "@types/jest": "^30.0.0",
    "@types/jscodeshift": "^0.11.10",
    "@types/node": "24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jscodeshift": "^0.15.1",
    "lighthouse": "^12.0.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "0.7.1",
    "sharp": "^0.34.3",
    "supabase": "^2.58.5",
    "tailwindcss": "^4.1.13",
    "ts-prune": "^0.10.3",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "js-yaml": ">=4.1.1",
    "glob": "^11.0.4"
  }
}

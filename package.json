{
  "name": "prepflow-landing",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "lint-staged",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "node scripts/analyze-bundle.js",
    "lighthouse": "lhci autorun",
    "lighthouse:mobile": "lighthouse http://localhost:3000 --view --emulated-form-factor=mobile",
    "lighthouse:desktop": "lighthouse http://localhost:3000 --view --emulated-form-factor=desktop",
    "perf:test": "npm run build && npm run lighthouse",
    "perf:analyze": "npm run build && npm run analyze",
    "perf:optimize": "node scripts/optimize-performance.js",
    "bundle:analyze": "node scripts/analyze-bundle.js",
    "bundle:webpack": "ANALYZE=true npm run build",
    "audit:deps": "node scripts/audit-dependencies.js",
    "bundle:size": "npm run build && npm run analyze"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.76.1",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-query-devtools": "^5.87.4",
    "@vercel/analytics": "^1.5.0",
    "date-fns": "^4.1.0",
    "exceljs": "^4.4.0",
    "gtag": "^1.0.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "next": "^16.0.0",
    "next-auth": "^4.24.13",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "stripe": "^19.2.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.3",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.13",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.0.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "sharp": "^0.34.3",
    "tailwindcss": "^4.1.13",
    "ts-jest": "^29.2.5",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}

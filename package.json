{
  "name": "curbos",
  "version": "0.1.1",
  "private": true,
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:legacy": "node server.js",
    "dev:http": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "lint-staged",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:accessibility": "node scripts/test-accessibility.js",
    "analyze": "node scripts/analyze-bundle.js",
    "lighthouse": "lhci autorun",
    "lighthouse:mobile": "lighthouse http://localhost:3000 --view --emulated-form-factor=mobile",
    "lighthouse:desktop": "lighthouse http://localhost:3000 --view --emulated-form-factor=desktop",
    "perf:test": "npm run build && npm run lighthouse",
    "perf:analyze": "npm run build && npm run analyze",
    "bundle:analyze": "node scripts/analyze-bundle.js",
    "bundle:webpack": "ANALYZE=true npm run build",
    "audit:deps": "node scripts/audit-dependencies.js",
    "bundle:size": "npm run build && npm run analyze",
    "lint:filesize": "node scripts/check-file-sizes.js",
    "changelog": "node scripts/generate-changelog.js",
    "detect-breakpoints": "node scripts/detect-breakpoints.js",
    "codemod:breakpoints": "npx jscodeshift -t scripts/codemods/breakpoint-migration.js app components",
    "codemod:breakpoints:write": "npx jscodeshift -t scripts/codemods/breakpoint-migration.js --write app components",
    "codemod:console": "npx jscodeshift -t scripts/codemods/console-migration.js app components",
    "codemod:console:write": "npx jscodeshift -t scripts/codemods/console-migration.js --write app components",
    "codemod:colors": "npx jscodeshift -t scripts/codemods/color-migration.js app/webapp components --parser tsx",
    "codemod:colors:write": "npx jscodeshift -t scripts/codemods/color-migration.js --write app/webapp components --parser tsx",
    "migrate:colors": "node scripts/migrate-colors.js",
    "migrate:colors:write": "node scripts/migrate-colors.js --write",
    "codemod:all": "npm run codemod:breakpoints && npm run codemod:console && npm run codemod:colors",
    "codemod:database-patterns": "npx jscodeshift -t scripts/codemods/database-patterns-migration.js app/api lib --parser tsx",
    "codemod:database-patterns:write": "npx jscodeshift -t scripts/codemods/database-patterns-migration.js --write app/api lib --parser tsx",
    "fix:database-patterns": "node scripts/fix-database-patterns-batch.js",
    "fix:database-patterns:write": "node scripts/fix-database-patterns-batch.js --fix",
    "fix:error-handling": "node scripts/fix-missing-error-handling.js",
    "fix:error-handling:write": "node scripts/fix-missing-error-handling.js --write",
    "cleanup": "node scripts/cleanup.js",
    "cleanup:check": "node scripts/cleanup.js --check",
    "cleanup:fix": "node scripts/cleanup.js --fix",
    "cleanup:report": "node scripts/cleanup.js --report",
    "cleanup:all": "node scripts/cleanup.js --check --fix --report",
    "cleanup:staged": "node scripts/cleanup.js --staged",
    "check:voice": "node scripts/check-voice-consistency.js",
    "curbos:status": "node scripts/protect-curbos.js status",
    "curbos:reset": "node scripts/protect-curbos.js reset",
    "curbos:unstage": "node scripts/protect-curbos.js unstage",
    "pre-deploy": "bash scripts/pre-deploy-check.sh",
    "diagnose:build": "node scripts/diagnose-vercel-build.js",
    "auth0:validate-env": "node scripts/validate-auth0-env.js",
    "auth0:check-config": "node scripts/check-auth0-config.js",
    "auth0:check-issues": "node scripts/check-auth0-issues.js",
    "auth0:update-config": "node scripts/update-auth0-config.js",
    "auth0:show-urls": "node scripts/show-auth0-urls.js",
    "auth0:validate": "npm run auth0:validate-env && npm run auth0:check-config && npm run auth0:check-issues",
    "vercel:check-env": "node scripts/check-vercel-env.js",
    "vercel:check-env-vars": "node scripts/check-vercel-env-vars.js",
    "vercel:set-env": "node scripts/set-vercel-env.js",
    "find-allergen-conflicts": "node scripts/find-allergen-vegan-conflicts.js",
    "audit:ids": "node scripts/audit-ids.js",
    "generate:background": "node scripts/generate-background-png.js",
    "supabase:status": "npx supabase status",
    "supabase:migrate": "npx supabase db push",
    "supabase:migrate:reset": "npx supabase db reset",
    "supabase:migrate:clean": "npx supabase migration repair --status reverted",
    "supabase:link": "npx supabase link --project-ref $(echo $NEXT_PUBLIC_SUPABASE_URL | sed 's|https://\\([^.]*\\)\\.supabase\\.co|\\1|')",
    "supabase:diff": "npx supabase db diff",
    "supabase:generate": "npx supabase migration new",
    "supabase:migrate:cleaning": "bash scripts/supabase-migrate-cleaning.sh",
    "stripe:setup": "node scripts/setup-stripe-env.js",
    "stripe:validate": "node scripts/validate-stripe-setup.js",
    "test:integration": "node scripts/test-auth0-stripe-integration.js",
    "test:auth0": "node scripts/test-auth0-comprehensive.js",
    "test:logout": "node scripts/test-logout-flow.js",
    "supabase:verify:cleaning": "node scripts/verify-cleaning-migration.js",
    "storage:create-buckets": "node scripts/create-storage-buckets.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.13.3",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@huggingface/inference": "^4.13.4",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.78.0",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-query-devtools": "^5.87.4",
    "@types/html2canvas": "^0.5.35",
    "@types/qrcode": "^1.5.6",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "auth0": "^4.7.0",
    "canvas-confetti": "^1.9.4",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "googleapis": "^166.0.0",
    "gtag": "^1.0.1",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.553.0",
    "next": "^16.0.7",
    "papaparse": "^5.4.1",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-icons": "^5.5.0",
    "react-qr-code": "^2.0.18",
    "react-signature-canvas": "^1.0.7",
    "recharts": "^2.8.0",
    "signature_pad": "^4.2.0",
    "square": "^43.2.1",
    "stripe": "^19.2.0",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "16.0.7",
    "@playwright/test": "^1.48.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/canvas-confetti": "^1.9.0",
    "@types/jest": "^30.0.0",
    "@types/jscodeshift": "^0.11.10",
    "@types/node": "24.9.2",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.7",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jscodeshift": "^0.15.1",
    "lighthouse": "^12.0.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "0.7.1",
    "sharp": "^0.34.3",
    "supabase": "^2.58.5",
    "tailwindcss": "^4.1.13",
    "ts-prune": "^0.10.3",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "js-yaml": ">=4.1.1",
    "glob": "^11.0.4"
  }
}

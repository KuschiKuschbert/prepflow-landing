    "@tanstack/react-query-devtools": "^5.87.4",
    "@vercel/analytics": "^1.5.0",
    "gtag": "^1.0.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "next": "15.4.6",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.2.0",
    "web-vitals": "^5.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.3",

    "stripe:validate": "node scripts/validate-stripe-setup.js",
    "test:integration": "node scripts/test-auth0-stripe-integration.js",
    "test:auth0": "node scripts/test-auth0-comprehensive.js",
    "test:logout": "node scripts/test-logout-flow.js",
    "supabase:verify:cleaning": "node scripts/verify-cleaning-migration.js",
    "storage:create-buckets": "node scripts/create-storage-buckets.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "scrape:recipes": "tsx scripts/recipe-scraper/index.ts",
    "scrape:recipes:allrecipes": "tsx scripts/recipe-scraper/index.ts --source allrecipes",
    "scrape:recipes:bbc": "tsx scripts/recipe-scraper/index.ts --source bbc-good-food",
    "scrape:recipes:food-network": "tsx scripts/recipe-scraper/index.ts --source food-network",
    "scrape:recipes:stats": "tsx scripts/recipe-scraper/index.ts --stats",
    "scrape:recipes:search": "tsx scripts/recipe-scraper/index.ts --search",

    "error:capture-build": "node scripts/error-capture/capture-build-errors.js",
    "error:detect-fixes": "node scripts/error-learning/detect-fixes.js",
    "error:document": "node scripts/error-learning/document-fix-interactive.js",
    "error:auto-document": "node scripts/error-learning/auto-document-fix.js",
    "error:report": "node scripts/error-learning/generate-learning-report.js",
    "error:generate-rules": "node -e \"const { generateRulesFromKnowledgeBase } = require('./lib/error-learning/rule-generator.ts'); generateRulesFromKnowledgeBase().catch(console.error);\"",
    "audit:any": "tsx scripts/audit-any.ts",
    "audit:dead-code": "node scripts/cleanup/checks/dead-code.js",
    "audit:scripts": "eslint scripts/",
    "check:coverage": "tsx scripts/coverage-scanner.ts",
    "check:bundle": "tsx scripts/bundle-budget.ts",
    "audit:unused": "tsx scripts/audit-dependencies.ts",
    "backfill:tags": "tsx scripts/backfill-ingredient-tags.ts",
    "backfill:fts": "tsx scripts/fix-fts.ts",
    "check:adr": "tsx scripts/adr-guard.ts",
    "sync:kb": "tsx scripts/kb-sync.ts",
    "adr:new": "cp docs/adr/TEMPLATE.md docs/adr/$(date +%Y-%m-%d)-new-record.md",
    "rsi:status": "node scripts/rsi/status.js",
    "rsi:rollback": "node scripts/rsi/rollback.js",
    "rsi:fix": "node scripts/rsi/auto-fix.js",
    "rsi:optimize": "node scripts/rsi/self-optimize.js",
    "rsi:evolve": "node scripts/rsi/evolve-rules.js",
    "rsi:refactor": "node scripts/rsi/auto-refactor.js",
    "rsi:rule": "node scripts/rsi/auto-rule.js",
    "rsi:run": "node scripts/rsi/orchestrator.js"

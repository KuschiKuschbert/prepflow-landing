{
  "name": "curbos",
  "version": "0.6.7",
  "private": true,
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:legacy": "node server.js",
    "dev:http": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "lint-staged",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:accessibility": "node scripts/test-accessibility.js",
    "analyze": "node scripts/analyze-bundle.js",
    "lighthouse": "lhci autorun",
    "lighthouse:mobile": "lighthouse http://localhost:3000 --view --emulated-form-factor=mobile",
    "lighthouse:desktop": "lighthouse http://localhost:3000 --view --emulated-form-factor=desktop",
    "perf:test": "npm run build && npm run lighthouse",
    "perf:analyze": "npm run build && npm run analyze",
    "bundle:analyze": "node scripts/analyze-bundle.js",
    "bundle:webpack": "ANALYZE=true npm run build",
    "audit:deps": "node scripts/audit-dependencies.js",
    "bundle:size": "npm run build && npm run analyze",
    "lint:filesize": "node scripts/check-file-sizes.js",
    "changelog": "node scripts/generate-changelog.js",
    "detect-breakpoints": "node scripts/detect-breakpoints.js",
    "codemod:breakpoints": "npx jscodeshift -t scripts/codemods/breakpoint-migration.js app components",
    "codemod:breakpoints:write": "npx jscodeshift -t scripts/codemods/breakpoint-migration.js --write app components",
    "codemod:console": "npx jscodeshift -t scripts/codemods/console-migration.js app components",

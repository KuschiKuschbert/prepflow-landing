    "scrape:recipes:allrecipes": "tsx lib/recipes/cli.ts --source allrecipes",
    "scrape:recipes:bbc": "tsx lib/recipes/cli.ts --source bbc-good-food",
    "scrape:recipes:food-network": "tsx lib/recipes/cli.ts --source food-network",
    "scrape:recipes:stats": "tsx lib/recipes/cli.ts --stats",
    "scrape:recipes:search": "tsx lib/recipes/cli.ts --search",

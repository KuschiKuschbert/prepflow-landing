    "cleanup:fix": "node scripts/cleanup.js --fix",
    "cleanup:report": "node scripts/cleanup.js --report",
    "cleanup:all": "node scripts/cleanup.js --check --fix --report",
    "cleanup:staged": "node scripts/cleanup.js --staged",
    "check:voice": "node scripts/check-voice-consistency.js",
    "check:architecture": "tsx scripts/check-architecture.ts",
    "check:health": "tsx scripts/sentinel.ts",
    "check:security": "tsx scripts/auditor.ts",
    "generate:tests": "tsx scripts/test-generator.ts",
    "teleport": "./scripts/teleport.sh",
    "curbos:status": "node scripts/protect-curbos.js status",
    "curbos:reset": "node scripts/protect-curbos.js reset",
    "curbos:unstage": "node scripts/protect-curbos.js unstage",
    "pre-deploy": "bash scripts/pre-deploy-check.sh",
    "diagnose:build": "node scripts/diagnose-vercel-build.js",
    "auth0:validate-env": "node scripts/validate-auth0-env.js",

    "supabase:migrate": "npx supabase db push",
    "supabase:migrate:reset": "npx supabase db reset",
    "supabase:migrate:clean": "npx supabase migration repair --status reverted",
    "supabase:link": "npx supabase link --project-ref $(echo $NEXT_PUBLIC_SUPABASE_URL | sed 's|https://\\([^.]*\\)\\.supabase\\.co|\\1|')",
    "supabase:diff": "npx supabase db diff",
    "supabase:generate": "npx supabase migration new",
    "supabase:migrate:cleaning": "bash scripts/supabase-migrate-cleaning.sh",
    "stripe:setup": "node scripts/setup-stripe-env.js",
    "stripe:validate": "node scripts/validate-stripe-setup.js",
    "test:integration": "node scripts/test-auth0-stripe-integration.js",
    "test:auth0": "node scripts/test-auth0-comprehensive.js",
    "test:logout": "node scripts/test-logout-flow.js",
    "supabase:verify:cleaning": "node scripts/verify-cleaning-migration.js",
    "storage:create-buckets": "node scripts/create-storage-buckets.js",
